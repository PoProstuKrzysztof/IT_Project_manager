@using IT_Project_manager;
@model List<Member>

@{
    ViewData["Title"] = "Members list";
}
<h1> Members</h1><br />

<table class="table">

    <tr>
        <th><b>ID</b></th>
        <th><b>Email</b></th>
        <th><b>Name</b></th>
        <th><b>Surname</b></th>
        <th><b>Date of birth</b></th>

    </tr>
    @foreach (var member in @Model)
    {

        <tr>
            
            <td>@member.Id</td>            
            <td>@member.Name</td>
            <td>@member.Surname</td>
            <td>@member.Email</td>
            <td>@member.DateOfBirth</td>
            <td><button class="btn-primary" asp-route-id="@member.Id">Edit</button></td>
            <td><button class="btn-danger" asp-route-id="@member.Id">Delete</button></td>
        </tr>

    }
</table>
